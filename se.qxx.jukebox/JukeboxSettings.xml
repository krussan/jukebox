<?xml version="1.0" encoding="UTF-8"?>
<JukeboxListenerSettings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="JukeboxSettings.xsd">
  <catalogs>
    <catalog includeSubdirectories="true" path="/c/media/BitTorrent">
      <vlcpaths>
      	<vlcpath player="vlc_main" path="file:///media/ultra/BitTorrent" />
      	<vlcpath player="black_hell" path="file:///Z:/BitTorrent" />
      </vlcpaths>
      <extensions>
        <extension value="avi"/>
        <extension value="mkv"/>
        <extension value="mp4"/>
      </extensions>
    </catalog>
  </catalogs>
  <subFinders subsPath="/c/media/Subs" threadWaitSeconds="25">
    <subFinder class="se.qxx.jukebox.subtitles.UndertexterSe">
    	<subFinderSettings>
    		<setting>
    			<key>regex</key>
    			<value><![CDATA[<tr[^>]*?>.*?<td[^>]*?>.*?<a[^>]*?alt\s*=\s*"Ladda\sner\sundertext[^"]*?".*?href\s*=\s*\"([^\"]*?)\"[^>]*?>.*?Nedladdningar.*?<br>\s*([^<]*?)</td[^>]*?>.*?</tr[^>]*?>]]></value>    			   		
			</setting>
			<setting>
			    <key>urlRegexGroup</key>
			    <value>1</value>
			</setting>
			<setting>
			    <key>nameRegexGroup</key>
			    <value>2</value>
			</setting>			
    		<setting>
    			<key>url</key>
    			<value><![CDATA[http://www.undertexter.se/?p=soek&add=arkiv&str=__searchString__]]></value>
    		</setting>
    	</subFinderSettings>
    </subFinder>
  </subFinders>
  <stringSplitters>
    <exclude>
    	<regex>
    		<![CDATA[]]>
    	</regex>
    </exclude>
    <parts>
        <partPattern groupPart="2">
	        <regex>
	            <![CDATA[(Part|CD)[.|_|\s|-]*(\d+)]]>
	        </regex>            
        </partPattern>
    </parts>
    <episodes>
        <pattern>
            <groups>
                <group property="season" id="1" />
                <group property="episode" id="2" />
            </groups>
	        <regex>
	            <![CDATA[s(\d{1,2})[.|_|\s|-]*e(\d{1,2})]]>
	        </regex>            
        </pattern>
        <pattern>
            <groups>
                <group property="season" id="1" />
                <group property="episode" id="2" />
            </groups>
	        <regex>
	            <![CDATA[\d{1,2}[.|_|\s|-]*sx[.|_|\s|-]*s\d{1,2}]]>
	        </regex>            
        </pattern>
        <pattern>
            <groups>
                <group property="season" id="1" />
                <group property="episode" id="2" />
            </groups>
            <regex>
                <![CDATA[(\d{1,2})[.|_|\s|-]*sezon[.|_|\s|-]*(\d{1,2})[.|_|\s|-]*seriya]]>
            </regex>
        </pattern>
    </episodes>
  	<splitter>
  	    <groups>
  	        <group property="title" id="1"/>
  	        <group property="year" id="4" />
    	    <group property="type" id="7" />
  	        <group property="format" id="9" />
  	        <group property="sound" id="11" />
  	      	<group property="language" id="13" />
  	      	<group property="group" id="14" />  	        	        	        	        	        	        	        	        	        	        
  	    </groups>
  		<regex>
  		    <!-- Matches Batman.Under.The.Red.Hood.2010.720p.XviD.AC3-ViSiON.avi -->
  			<![CDATA[((\w[.|_|\s|-]*)*)([\[|(]?((19|20)\d{2})[\]|)]?)+[.|_|\s|-]*([\[|(]?(1080p|720p|dvdscreener|dvdscr|dvdrip|brrip|bdrip|ppvrip|ppv|telesync|ts|hdrip|r5\s*line|cam)[\]|)]?)?[.|_|\s|-]*([\[|(]?(xvid|x264|iflix|h264)[\]|)]?)?[.|_|\s|-]*([\[|(]?(ac3\s5\.1|ac3|aac)[\]|)]?)?[.|_|\s|-]*([\[|(]?(eng|swe)[\]|)]?)?[.|_|\s|-]*(.*)[.|_|\s|-]*]]>
  		</regex>
  		<ignore>
  		    <![CDATA[READNFO|CAM|LIMITED|SWESUB|UNRATED|REPACK|\d{2}K|UNRATED|V3|\d{1,3}MB|\d{1,3}M|HDtv|i2p|EXTENDED|RETAIL|[.|_|\s|-]DC|bluray|www\.\w\.\w\.\w]]>
  		</ignore>
  	</splitter>
  	<splitter>
    	<groups>
  	        <group property="title" id="1"/>
    	    <group property="type" id="3" />
  	        <group property="format" id="5" />
  	        <group property="sound" id="7" />
  	      	<group property="language" id="9" />
  	      	<group property="group" id="10" />  	        	        	        	        	        	        	        	        	        	        
  	    </groups> 	    	    
  		<regex>
  			<![CDATA[((\w[.|_|\s|-]*)*)[\[|(]?(1080p|720p|dvdscreener|dvdscr|dvdrip|brrip|bdrip|ppvrip|ppv|telesync|ts|hdrip|r5\s*line|cam)[\]|)]?[.|_|\s|-]*([\[|(]?(xvid|x264|iflix|h264)[\]|)]?)?[.|_|\s|-]*([\[|(]?(ac3\s5\.1|ac3|aac)[\]|)]?)?[.|_|\s|-]*([\[|(]?(eng|swe)[\]|)]?)?[.|_|\s|-]*(.*)]]>
  		</regex>
  		<ignore>
  		    <![CDATA[READNFO|CAM|LIMITED|SWESUB|UNRATED|REPACK|\d{2}K|UNRATED|V3|\d{1,3}MB|\d{1,3}M|HDtv|i2p|EXTENDED|RETAIL|[.|_|\s|-]DC|bluray|www\.\w\.\w\.\w]]>
  		</ignore>
  	</splitter>
    <splitter>
        <groups>
            <group property="title" id="1" />
        </groups>
    	<regex>
    		<![CDATA[(.*)]]>
    	</regex>
  		<ignore>
  		    <![CDATA[READNFO|CAM|LIMITED|SWESUB|UNRATED|REPACK|\d{2}K|UNRATED|V3|\d{1,3}MB|\d{1,3}M|HDtv|i2p|EXTENDED|RETAIL|1080p|720p|DVDScr|DVDSCR|DvDRip|DVDRip|BRRip|BDRip|PPVRIP|PPV|TELESYNC|TS|HDRip|R5\s*LINE|R5\s*LiNE|R5\s*Line|CAM|Xvid|XviD|XViD|XvID|XVID|x264|IFLIX|AC3\s5\.1|AC3|[.|_|\s|-]DC|bluray|www\.\w\.\w\.\w|h264|aac]]>
  		</ignore>
    </splitter>
  </stringSplitters>
  <builders>
      <builder class="se.qxx.jukebox.builders.FilenameBuilder" enabled="true" weight="1" />
      <builder class="se.qxx.jukebox.builders.ParentDirectoryBuilder" enabled="true" weight="1" />
      <builder class="se.qxx.jukebox.builders.NfoBuilder" enabled="true" weight="1" />      
  </builders>
  <tcpListener>
    <port value="2152"/>
  </tcpListener>
  <vlc>
  	<server 
  	    name="vlc_main"
  		host="192.168.1.130" 
  		broadcastAddress="192.168.1.255"
  		port="2150"
  		subsPath="file:///media/ultra/Subs"
  		macAddress="00-17-42-BE-FC-36"
  		hibernatorPort="2148"  		
 	/>	
   	<server 
  	    name="black_hell"
  		host="192.168.1.100" 
  		broadcastAddress="192.168.1.255"
  		port="2150"
  		subsPath="file:///Z:/Subs"
  		macAddress="00-1E-8C-1F-94-ED"
  		hibernatorPort="2148"  		
 	/>		
  </vlc>
  <logs>
  	<log type="file" filename="/c/media/Dev/src/jukebox/jukebox.log" level="DEBUG" logs="MAIN" />
  	<log type="file" filename="/c/media/Dev/src/jukebox/subtitle.log" level="DEBUG" logs="SUBS" />
  	<log type="file" filename="/c/media/Dev/src/jukebox/watcher.log" level="DEBUG" logs="FIND" />
  	<log type="file" filename="/c/media/Dev/src/jukebox/comm.log" level="DEBUG" logs="COMM" />
  	<log type="file" filename="/c/media/Dev/src/jukebox/upgrade.log" level="DEBUG" logs="UPGRADE" />
  </logs>  
</JukeboxListenerSettings>
