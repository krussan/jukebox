<?xml version="1.0" encoding="UTF-8"?>
<JukeboxListenerSettings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="JukeboxSettings.xsd">
  <catalogs>
    <catalog includeSubdirectories="true" path="/c/media/BitTorrent">
      <localPaths>
      	<path player="vlc_main" path="file:///media/ultra/BitTorrent" />
      	<path player="black_hell_front" path="file:///Z:/BitTorrent" />
      </localPaths>
      <extensions>
        <extension value="avi"/>
        <extension value="mkv"/>
        <extension value="mp4"/>
      </extensions>
    </catalog>
    <catalog includeSubdirectories="true" path="F:/jukeboxDev">
      <localPaths>
      	<path player="vlc_main" path="file:///media/ultra/BitTorrent" />
      	<path player="black_hell_front" path="file:///Z:/BitTorrent" />
      </localPaths>
      <extensions>
        <extension value="avi"/>
        <extension value="mkv"/>
        <extension value="mp4"/>
      </extensions>
    </catalog>
	<catalog includeSubdirectories="true" path="/media/ultra/BitTorrent/House of Cards - Seasons 1-3/Season 1">
      <localPaths>
      	<path player="vlc_main" path="file:///media/ultra/BitTorrent" />
      	<path player="black_hell_front" path="file:///Z:/BitTorrent" />
      	<path player="ubuntu_dev" path="/media/ultra/BitTorrent" />
      </localPaths>
      <extensions>
        <extension value="avi"/>
        <extension value="mkv"/>
        <extension value="mp4"/>
      </extensions>
    </catalog>        
  </catalogs>
  <subFinders subsPath="/c/media/Subs" threadWaitSeconds="25">
	<subFinder class="se.qxx.jukebox.subtitles.Subscene" enabled="true">
    	<subFinderSettings>
    		<setting>
    			<key>searchResultRegex</key>
    			<value><![CDATA[<a\s*href=\"(/subtitles/[^\"]*)\">[^<]*</a>]]></value>    			   		
			</setting>
			<setting>
			    <key>searchResultUrlGroup</key>
			    <value>1</value>
			</setting>
			<setting>
			    <key>searchResultUrlGroup</key>
			    <value>1</value>
			</setting>
			<setting>
				<key>listResultRegex</key>
				<value><![CDATA[<td[^>]*>\s*<a\s*href="(\/subtitles[^"]*)">\s*<span[^>]*>([^<]*)</span>\s*<span[^>]*>([^<]*)</span>\s*<\/a>]]></value>
			</setting>
			<setting>
			    <key>listResultUrlGroup</key>
			    <value>1</value>
			</setting>
			<setting>
			    <key>listResultLanguageGroup</key>
			    <value>2</value>
			</setting>
			<setting>
			    <key>listResultNameGroup</key>
			    <value>3</value>
			</setting>
			<setting>
				<key>downloadUrlRegex</key>
				<value><![CDATA[<a\s*href="([^"]*)"[^>]*>\s*Download\s*[a-z]*\ssubtitle\s*.*?<\/a>]]></value>
			</setting>
			<setting>
			    <key>downloadUrlGroup</key>
			    <value>1</value>
			</setting>			
    		<setting>
    			<key>url</key>
    			<value><![CDATA[https://subscene.com/subtitles/title?q=__searchString__]]></value>
    		</setting>
    	</subFinderSettings>
    </subFinder>    
  </subFinders>
  <builders>
      <builder class="se.qxx.jukebox.builders.FilenameBuilder" enabled="false" weight="1" />
      <builder class="se.qxx.jukebox.builders.ParentDirectoryBuilder" enabled="true" weight="1" />
      <builder class="se.qxx.jukebox.builders.NfoBuilder" enabled="true" weight="1" />      
      <builder class="se.qxx.jukebox.builders.ParserBuilder" enabled="true" weight="1" />
  </builders>
  <tcpListener>
    <port value="2152"/>
  </tcpListener>
  <players>
  	<server 
  	    name="vlc_main"
  		host="192.168.1.130" 
  		broadcastAddress="192.168.1.255"
  		port="2150"
  		subsPath="file:///media/ultra/Subs"
  		macAddress="00-17-42-BE-FC-36"
  		hibernatorPort="2148"  	
  		type="VLC"	
 	/>	
   	<server 
  	    name="black_hell_front"
  		host="192.168.1.100" 
  		broadcastAddress="192.168.1.255"
  		port="2156"
  		subsPath="file:///Z:/Subs"
  		macAddress="00-1E-8C-1F-94-ED"
  		hibernatorPort="2148"
  		type="JukeboxFront"  		
 	/>		
   	<server 
  	    name="living_room_chromecast"
  		host="192.168.1.255" 
  		broadcastAddress="192.168.1.255"
  		port="2156"
  		subsPath="smb://192.168.1.120/media/Subs"
  		macAddress=""
  		hibernatorPort="2148"
  		type="Chromecast"  		
 	/>	 	
  </players>
  <logs>
  	<log type="file" filename="jukebox.log" level="DEBUG" logs="MAIN" />
  	<log type="file" filename="subtitle.log" level="DEBUG" logs="SUBS" />
  	<log type="file" filename="watcher.log" level="DEBUG" logs="FIND" />
  	<log type="file" filename="comm.log" level="DEBUG" logs="COMM" />
  	<log type="file" filename="upgrade.log" level="DEBUG" logs="UPGRADE" />
  	<log type="file" filename="imdb.log" level="DEBUG" logs="IMDB" />	
  	<log type="file" filename="webserver.log" level="DEBUG" logs="WEBSERVER" />
  	<!-- <log type="file" filename="db.log" level="DEBUG" logs="DB" /> -->
  </logs>  
</JukeboxListenerSettings>
