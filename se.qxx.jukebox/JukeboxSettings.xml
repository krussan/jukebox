<?xml version="1.0" encoding="UTF-8"?>
<JukeboxListenerSettings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="JukeboxSettings.xsd">
  <catalogs>
    <catalog includeSubdirectories="true" path="/c/media/BitTorrent">
      <localPaths>
      	<path player="vlc_main" path="file:///media/ultra/BitTorrent" />
      	<path player="black_hell_front" path="file:///Z:/BitTorrent" />
      </localPaths>
      <extensions>
        <extension value="avi"/>
        <extension value="mkv"/>
        <extension value="mp4"/>
      </extensions>
    </catalog>
    <catalog includeSubdirectories="true" path="F:/jukeboxDev">
      <localPaths>
      	<path player="vlc_main" path="file:///media/ultra/BitTorrent" />
      	<path player="black_hell_front" path="file:///Z:/BitTorrent" />
      </localPaths>
      <extensions>
        <extension value="avi"/>
        <extension value="mkv"/>
        <extension value="mp4"/>
      </extensions>
    </catalog>
	<catalog includeSubdirectories="true" path="/media/ultra/BitTorrent/House of Cards - Seasons 1-3/Season 1">
      <localPaths>
      	<path player="vlc_main" path="file:///media/ultra/BitTorrent" />
      	<path player="black_hell_front" path="file:///Z:/BitTorrent" />
      	<path player="ubuntu_dev" path="/media/ultra/BitTorrent" />
      </localPaths>
      <extensions>
        <extension value="avi"/>
        <extension value="mkv"/>
        <extension value="mp4"/>
      </extensions>
    </catalog>        
  </catalogs>
  <subFinders subsPath="/c/media/Subs" threadWaitSeconds="25">
    <subFinder class="se.qxx.jukebox.subtitles.UndertexterSe">
    	<subFinderSettings>
    		<setting>
    			<key>regex</key>
    			<value><![CDATA[<tr[^>]*?>.*?<td[^>]*?>.*?<a[^>]*?alt\s*=\s*"Ladda\sner\sundertext[^"]*?".*?href\s*=\s*\"([^\"]*?)\"[^>]*?>.*?Nedladdningar.*?<br>\s*([^<]*?)</td[^>]*?>.*?</tr[^>]*?>]]></value>    			   		
			</setting>
			<setting>
			    <key>urlRegexGroup</key>
			    <value>1</value>
			</setting>
			<setting>
			    <key>nameRegexGroup</key>
			    <value>2</value>
			</setting>			
    		<setting>
    			<key>url</key>
    			<value><![CDATA[http://www.undertexter.se/?p=soek&add=arkiv&str=__searchString__]]></value>
    		</setting>
    	</subFinderSettings>
    </subFinder>
    <subFinder class="se.qxx.jukebox.subtitles.EngsubNet">
    	<subFinderSettings>
    		<setting>
    			<key>regex</key>
    			<value><![CDATA[<tr>.*?<td[^>]*>.*?<a[^>]*href="([^"]*)"[^>]*>\s*<img\s*src="http://engsub.net/bilder/ladda_codec_mini.gif"[^>]*>\s*</a>.*?Downloads:\s*\d*\s*<br>\s*<img[^>]*>\s*<br>\s*(.*?)</td>]]></value>    			   		
			</setting>
			<setting>
			    <key>urlRegexGroup</key>
			    <value>1</value>
			</setting>
			<setting>
			    <key>nameRegexGroup</key>
			    <value>2</value>
			</setting>			
    		<setting>
    			<key>url</key>
    			<value><![CDATA[http://www.engsub.net/?p=eng_search&add=arkiv&str=__searchString__]]></value>
    		</setting>
    	</subFinderSettings>
    </subFinder>
  </subFinders>
  <builders>
      <builder class="se.qxx.jukebox.builders.FilenameBuilder" enabled="false" weight="1" />
      <builder class="se.qxx.jukebox.builders.ParentDirectoryBuilder" enabled="true" weight="1" />
      <builder class="se.qxx.jukebox.builders.NfoBuilder" enabled="true" weight="1" />      
      <builder class="se.qxx.jukebox.builders.ParserBuilder" enabled="true" weight="1" />
  </builders>
  <tcpListener>
    <port value="2152"/>
  </tcpListener>
  <players>
  	<server 
  	    name="vlc_main"
  		host="192.168.1.130" 
  		broadcastAddress="192.168.1.255"
  		port="2150"
  		subsPath="file:///media/ultra/Subs"
  		macAddress="00-17-42-BE-FC-36"
  		hibernatorPort="2148"  	
  		type="VLC"	
 	/>	
   	<server 
  	    name="black_hell_front"
  		host="192.168.1.100" 
  		broadcastAddress="192.168.1.255"
  		port="2156"
  		subsPath="file:///Z:/Subs"
  		macAddress="00-1E-8C-1F-94-ED"
  		hibernatorPort="2148"
  		type="JukeboxFront"  		
 	/>		
   	<server 
  	    name="living_room_chromecast"
  		host="192.168.1.255" 
  		broadcastAddress="192.168.1.255"
  		port="2156"
  		subsPath="smb://192.168.1.120/media/Subs"
  		macAddress=""
