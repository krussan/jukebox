<?xml version="1.0" encoding="UTF-8"?>
<parser xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="parser.xsd">
  <keywords>
    <type>
        <word key="480p" />
        <word key="720p" />
        <word key="1080p" />
        <word key="PPVRIP" />
        <word key="BRRip" />
        <word key="DVDRip" />
        <word key="VODRip" />
        <word key="HDRip" />
        <word key="WEBRip" />
        <word key="DVDSCREENER" />
        <word key="DVDSCR" />
        <word key="TS" />
        <word key="PPV" />
        <word key="Bluray" />
        <word key="BDRip" />
        <word key="HDTV" />
        <word key="18p" />        
        <word key="108p" />
    </type>
    <year>
        <word key="(19|20)\d{2}" regex="true" group="0" />
    </year>
    <parts>
        <word key="(Part|CD)[.|_|\s|-]*(\d+)" regex="true" group="2" lookahead="1" />
    </parts>
    <season>
        <word key="(\d{1,2})(\d{2})" regex="true" group="1" recursiveCount="2" firstToken="true" />
        <word key="s(\d{1,2})" regex="true" group="1" />
        <word key="Season(\d{1,2})" regex="true" group="1" />
        <word key="(\d{1,2})sezon" regex="true" group="1" />
        <word key="s(\d{1,2})e(\d{1,2})" regex="true" group="1" recursiveCount="2" />
        <word key="(\d{1,2})x(\d{1,2})" regex="true" group="1" recursiveCount="2" />
        <word key="(\d{1,2})sezon(\d{1,2})[.|_|\s|-]*seriya" regex="true" group="1" recursiveCount="2" />
        <word key="Season(\d{1,2})Episode(\d{1,2})" regex="true" group="1" recursiveCount="2" />
    </season>
    <episode>
        <word key="(\d{1,2})(\d{2})" regex="true" group="2" recursiveCount="2" firstToken="true" />
        <word key="e(\d{1,2}).*" regex="true" group="1" />
        <word key="(\d{1,2})seriya" regex="true" group="1" />
        <word key="Episode(\d{1,2})" regex="true" group="1" />
        <word key="s(\d{1,2})e(\d{1,2})" regex="true" group="2" />
        <word key="(\d{1,2})x(\d{1,2})" regex="true" group="2" />
        <word key="(\d{1,2})sezon(\d{1,2})seriya" regex="true" group="2" />
        <word key="Season(\d{1,2})Episode(\d{1,2})" regex="true" group="2" />
    </episode>
    <format>
        <word key="XviD" />
        <word key="IFLIX" />
        <word key="x264" />
        <word key="h264" />
    </format>
    <sound>
        <word key="AC3" />
        <word key="aac" />
        <word key="ac3pt" />
        <word key="dd51" />
        <word key="AAC2" />
    </sound>
    <language>
        <word key="Eng" />
        <word key="English" />
        <word key="SWEDISH" />
        <word key="SWE" />
        <word key="SWESUB" />
        <word key="German" />
        <word key="GER" />
        <word key="en" />
        <word key="de" />
    </language>
    <groups>
        <word key="tvp" />
        <word key="KILLERS" />
        <word key="LOL" />
        <word key="DIAMOND" />
        <word key="JYK" />
        <word key="MILLENIUM" />
        <word key="ASAP" />
        <word key="YIFY" />
        <word key="ETRG" />
        <word key="PLAYNOW" />
        <word key="SPARKS" />
        <word key="CM8" />
        <word key="tvs" />
    </groups>
    <other>
        <word key="REPACK" />
        <word key="V2" />
        <word key="M777" />
        <word key="READNFO" />
        <word key="HQ" />
        <word key="LIMITED" />
        <word key="MD" />
        <word key="i2p" />
        <word key="EXTENDED" />
        <word key="DC" />
        <word key="dl" />
        <word key="ded" />
        <word key="PROPER" /> 
        <word key="DUB" />  
        <word key="WEB" />
        <word key="rerip" />
        <word key="UNRATED" />     
    </other>
  </keywords>
</parser>
